{
  "name": "emr",
  "version": "1.1.0",
  "description": "Hospital Management System",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ./ormconfig.json",
    "typeorm:migration:generate": "yarn typeorm migration:generate -n",
    "typeorm:migration:create": "yarn typeorm migration:create -n",
    "typeorm:migration:run": "yarn typeorm migration:run",
    "typeorm:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "typeorm:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed --configName=ormconfig.json",
    "migrate": "yarn typeorm:migration:run && yarn typeorm:seed",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.100.0",
    "@aws-sdk/s3-request-presigner": "^3.100.0",
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/bull": "^0.5.4",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^8.4.4",
    "@nestjs/schedule": "^1.1.0",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^8.0.3",
    "@nestjs/websockets": "^8.4.4",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "bull": "^4.8.2",
    "cache-manager": "^3.6.1",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "dotenv": "^16.0.0",
    "exceljs": "^4.3.0",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "lodash.startcase": "^4.4.0",
    "moment": "^2.29.4",
    "mysql2": "^2.3.3",
    "numeral": "^2.0.6",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "puppeteer": "^13.7.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "say": "^0.16.0",
    "smsglobal": "^1.2.4",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.30",
    "typeorm-seeding": "^1.6.1",
    "zklib-js": "^1.3.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bull": "^3.15.8",
    "@types/cache-manager": "^3.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.2.5",
    "lint-staged": "^12.4.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": []
  }
}
